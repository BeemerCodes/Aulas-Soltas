algoritmo "Pratica01_Aula-03"
  var
  
     num1,num2,resposta: inteiro
     
  inicio

        escreval("Digite um numero: ")
        leia(num1)
        
        escreval("Digite outro numero: ")
        leia(num2)
        
        resposta <- (num1 + num2)
        
        escreva("A soma dos numeros ",num1," +",num2," gerou:",resposta)

fimalgoritmo