algoritmo "Ex-Real-para-Dolar-Aula-06"
//  
//  

  var
  
     valor_real, total: real


  inicio
  
        escreva("Quantos Reais você possui? ")
        leia(valor_real)
        
        total <- valor_real/5.60
        
        escreva("Você tem ", total:4:1, " Dolares disponiveis")



fimalgoritmo