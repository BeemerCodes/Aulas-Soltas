algoritmo "ComandosDeEntrada_Aula-03"
  var

     nome: caractere
     
  inicio
        escreval("---*---*---*---*---*---*---*---")
        escreva("Digite seu nome: ")
        leia(nome)
        escreval("Prazer em te conhecer " , nome)
        escreva("---*---*---*---*---*---*---*---")

fimalgoritmo